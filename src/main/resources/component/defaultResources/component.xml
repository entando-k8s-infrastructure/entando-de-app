<?xml version="1.0" encoding="UTF-8"?>
<component>
    <code>defaultResources</code>
    <description>Webapp default resources</description>
    <installation>
        <environment code="production">
            <defaultSqlResources>
                <datasource name="portDataSource">
                    classpath:sql/defaultResources/port_data_production.sql
                </datasource>
                <datasource name="servDataSource">
                    classpath:sql/defaultResources/serv_data_production.sql
                </datasource>
            </defaultSqlResources>
        </environment>
        <environment code="test">
            <defaultSqlResources>
                <datasource name="portDataSource">
                    classpath:sql/defaultResources/port_data_test.sql
                </datasource>
                <datasource name="servDataSource">
                    classpath:sql/defaultResources/serv_data_test.sql
                </datasource>
            </defaultSqlResources>
        </environment>
    </installation>
</component>
